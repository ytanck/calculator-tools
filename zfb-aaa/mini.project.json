{
  "format": 2,
  "compileType": "mini",
  "compileOptions": {
    "component2": true,
    "typescript": false,
    "less": false,
    "enableNodeModuleBabelTransform": true,
    "nodeModulesES6Whitelist": [
      "json-circular-stringify",
      "@gyjx/fy-sdk"
    ]
  },
  "assetsInclude": [],
  "developOptions": {
    "hotReload": true
  },
