@font-face {
  font-family: "icon";
  src: url('data:font/ttf;charset=utf-8;base64,');
}
[class^="icon-"],
[class*=" icon-"] {
  font-family: 'icon' !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-add:before {
  content: "\ea01";
}
.icon-delete:before {
  content: "\ea02";
}
.icon-up:before {
  content: "\ea03";
}
.icon-xuanzhong:before {
  content: "\ea04";
}
.icon-zoom:before {
  content: "\ea05";
}
page {
  /* flex: 1; */
  /* display: flex; */
  background: #f2f2f2;
  overflow-x: hidden;
  font-family: PingFangSC-Regular;
}
image {
  vertical-align: top;
}
.empty-style {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.empty-style .empty-img {
  width: 326rpx;
  height: 203rpx;
  margin: 100rpx auto 0;
  background: url('');
  background-size: 326rpx 203rpx;
}
.empty-style .empty-msg {
  text-align: center;
  margin-top: 64rpx;
  color: #8a96a3;
  font-size: 28rpx;
}
.greyCol {
  color: #999999 !important;
}
.redCol {
  color: #ed4040 !important;
}
