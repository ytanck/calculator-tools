* {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}
.page-wraper {
  display: flex;
  width: 100vw;
  height: 100vh;
  flex-direction: column;
  align-items: center;
  background-color: rgba(253, 234, 238);
}
.item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  width: 702rpx;
  height: 110rpx;
  margin-top: 20rpx;
  border-radius: 20rpx;
  background-color: #fff;
}
.item .right {
  font-size: 26rpx;
  color: #aaa;
}
