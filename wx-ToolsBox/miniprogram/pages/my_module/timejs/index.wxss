.body {
    height: auto;
    width: 100%;
}

.body,.item {
    box-sizing: border-box;
}

.item {
    background: linear-gradient(90deg,#f055a8,#fe7d89);
    border-radius: 15rpx;
    margin-left: 3%;
    margin-top: 30rpx;
    padding: 40rpx 30rpx;
    width: 94%;
}

.add_item {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    margin-bottom: 30rpx;
}

.item_top {
    font-size: 35rpx;
    height: 80rpx;
    line-height: 80rpx;
    width: 100%;
}

.item_name,.item_time {
    float: left;
    height: 100%;
    width: 50%;
}

.item_time {
    font-size: 30rpx;
    text-align: right;
}

.item_bottm {
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    width: 100%;
}

.item_endTime {
    float: left;
    height: 100%;
    width: 80%;
}

.item_text {
    float: left;
    height: 100%;
    text-align: right;
    width: 20%;
}

.delBtn {
    background: red;
    border-radius: 10rpx 0 0 10rpx;
    color: #fff;
    height: 100%;
    left: 100%;
    line-height: 200rpx;
    position: absolute;
    text-align: center;
    top: 0;
    width: 150rpx;
}