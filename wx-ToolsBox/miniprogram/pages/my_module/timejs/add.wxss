.back {
    background: #fff;
    height: 150rpx;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 999;
}

.x {
    height: 180rpx;
}

.ht {
    height: 150rpx;
    width: 100%;
}

.back_img {
    color: #fff;
    height: 50rpx;
    left: 20rpx;
    position: absolute;
    top: 65%;
    transform: translateY(-50%);
    width: 50rpx;
}

.back_img image {
    height: 100%;
    width: 100%;
}

.title {
    line-height: 180rpx;
}

.title,.titlex {
    background: #3d3243;
    box-shadow: 0 2rpx 5rpx 5rpx #352c3a;
    color: #fff;
    font-size: 35rpx;
    height: 100%;
    text-align: center;
    width: 100%;
}

.titlex {
    line-height: 230rpx;
}

.body {
    box-sizing: border-box;
    padding: 20rpx 30rpx 0;
}

.body,.input_name {
    height: auto;
    width: 100%;
}

.input_name {
    margin-bottom: 10rpx;
}

.input_text {
    color: #fff;
    font-size: 40rpx;
    height: auto;
    width: 100%;
}

.input_box {
    border: 1rpx solid #dcdcdc;
    border-radius: 20rpx;
    color: #000;
    height: auto;
    padding: 20rpx;
}

.input_box,.time_box {
    box-sizing: border-box;
    width: 100%;
}

.time_box {
    border-bottom: 1rpx solid #dcdcdc;
    color: #3fb298;
    height: 80rpx;
    padding: 10rpx 5rpx;
}

.time_display {
    float: left;
    font-size: 30rpx;
    height: 100%;
    line-height: 60rpx;
    width: 80%;
}

.time_nongli {
    float: left;
    height: 100%;
    width: 20%;
}

.gongli {
    border: 1rpx solid #2ed8ff;
    color: #2ed8ff;
}

.gongli,.nongli {
    border-radius: 10rpx;
    box-sizing: border-box;
    font-size: 35rpx;
    height: auto;
    left: 20rpx;
    padding: 10rpx;
    position: relative;
    text-align: center;
    top: -5rpx;
    width: 100rpx;
}

.nongli {
    background: #2ed8ff;
    color: #fff;
}

.bg_box {
    height: 300rpx;
}

.bg_box,.text_box {
    border-radius: 20rpx;
    box-sizing: border-box;
    color: #fff;
    margin-top: 10rpx;
    padding: 0 5rpx;
    white-space: nowrap;
    width: 100%;
}

.text_box {
    height: 100rpx;
}

.bg_item {
    border-radius: 10rpx;
    display: inline-block;
    height: 100%;
    margin-right: 15rpx;
    position: relative;
    width: 200rpx;
}

.bg_box .custom {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 100% 100%;
}

.text_item {
    background: #fff;
    border-radius: 10rpx;
    display: inline-block;
    height: 100%;
    margin-right: 15rpx;
    position: relative;
    width: 100rpx;
}

.cur {
    background: #fff;
    border: 1rpx solid #666;
    border-radius: 50%;
    color: #000;
    font-size: 25rpx;
    height: 40rpx;
    left: 50%;
    line-height: 34rpx;
    position: absolute;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 40rpx;
}

.cur,.finishAdd {
    box-sizing: border-box;
    text-align: center;
}

.finishAdd {
    background: #1296db;
    border: 1rpx solid #2ed8ff;
    border-radius: 15rpx;
    color: #fff;
    font-size: 35rpx;
    height: 100rpx;
    line-height: 100rpx;
    margin-left: 50%;
    margin-top: 40rpx;
    transform: translateX(-50%);
    width: 400rpx;
}

.jumpModel {
    margin: 0 auto;
    position: fixed;
    top: 50%;
    transform: translateY(-50%);
    width: 100%;
    z-index: 2;
}

.jumpShow {
    display: block;
}

.jumpHide {
    display: none;
}

.jumpDetail {
    background-color: #fff;
    border-radius: 20rpx;
    box-sizing: border-box;
    margin: 0 auto;
    padding: 20rpx 80rpx;
    width: 600rpx;
}

.jumpDetail view {
    font-size: 30rpx;
    height: auto;
    padding: 50rpx 0;
    text-align: center;
    width: 100%;
}

.jumpDetail button {
    background: #ff4747;
    border-radius: 30rpx/50%;
    color: #fff;
    font-size: 31rpx;
    margin-bottom: 30rpx;
}

.jumpMask {
    background: rgba(0,0,0,.5);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 1;
}